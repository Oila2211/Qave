 useSelector } from \"react-redux\";\nimport { Row, Col, ListGroup, Image, Form, Button, Card, FormControl } from \"react-bootstrap\";\nimport { FaTrash } from \"react-icons/fa6\";\nimport Message from '../components/Message'\nimport { addToCart, removeFromCart } from \"../slices/cartSlice\";\n\nconst CartScreen = () => {\n    const navigate = useNavigate();\n    const dispatch = useDispatch();\n\n\n    const cart = useSelector((state) => state.cart);\n    const { cartItems } = cart;\n\n    const addToCartHandler = async (product, qty) => {\n        dispatch(addToCart({...product, qty}))\n    };\n\n    const removeFromCartHandler = async (id) => {\n        dispatch(removeFromCart(id))\n    };\n\n    const checkoutHandler = () => {\n        navigate('/login?redirect=/delivery');\n    }\n\n    const maxOrderQuantity = 10; // Adjust qty number as pleased...\n  return (\n    <Row>\n        <Col md={8}>\n            <h1 style={{marginBottom: '20px'}}>Shopping Cart</h1>\n            {cartItems.length === 0 ? (\n                <Message>\n                    Your Cart is empty <Link to='/'>Go Back</Link>\n                </Message>\n            ) : (\n                <ListGroup variant=\"flush\">\n                    {cartItems.map((item) => (\n                        <ListGroup.Item key={item._id}>\n                            <Row>\n                                <Col md={2}>\n                                    <Image src={item.Image} alt={item.name} fluid rounded />\n                                </Col>\n                                <Col md={3}>\n                                    <Link to={`/product/${item._id}`}>{item.name}</Link>\n                                </Col>\n                                <Col>{item.price}</Col>\n                                <Col md={2}>\n                                <FormControl\n                                    as='select'\n                                    value={item.qty}\n                                    onChange={(e) => addToCartHandler(item, Number(e.target.value))}>\n                                    {[...Array(maxOrderQuantity).keys()].map(x => (\n                                        <option key={x + 1} value={x + 1}>\n                                            {x + 1}\n                                        </option>\n                                    ))}\n                                </FormControl>\n                                </Col>\n                                <Col md={2}>\n                                    <Button type=\"button\" variant=\"light\" onClick={ () =>\n                                    removeFromCartHandler(item._id)}>\n                                        <FaTrash/>\n                                    </Button>\n                                </Col>\n                            </Row>\n                        </ListGroup.Item>\n                    ))}\n                </ListGroup>\n            ) }\n        </Col>\n        <Col md={4}>\n            <Card>\n                <ListGroup variant=\"flush\">\n                    <ListGroup.Item>\n                        <h2>\n                            Subtotal ({ cartItems.reduce((acc, item) => acc + item.qty, 0)})\n                            items\n                        </h2>\n                        SEK{ cartItems.reduce((acc, item) => acc + item.qty * item.price, 0).toFixed(2) }\n                    </ListGroup.Item>\n                    <ListGroup.Item>\n                        <Button type=\"button\" className=\"btn-block\"\n                         disabled={cartItems.length === 0}\n                         onClick={checkoutHandler}>\n                            Proceed To CheckOut\n                        </Button>\n                    </ListGroup.Item>\n                </ListGroup>\n            </Card>\n        </Col>\n    </Row>\n  )\n}\n\nexport default CartScreen;","/Users/mac/Afriqana/frontend/src/slices/authSlice.js",["180"],[],"import { createSlice } from '@reduxjs/toolkit';\n\nconst initialState = {\n    userInfo: localStorage.getItem('userInfo') ? JSON.parse(localStorage.\n    getItem('userInfo')) : null,\n}\n\nconst authSlice = createSlice({\n    name: 'auth',\n    initialState,\n    reducers: {\n        setCredentials: (state, action) => {\n            state.userInfo = action.payload;\n            localStorage.setItem('userInfo', JSON.stringify(action.payload))\n        },\n        logout: (state, action) => {\n            state.userInfo = null;\n            localStorage.removeItem('userInfo')\n        }\n    }\n})\n\nexport const { setCredentials, logout } = authSlice.actions;\n\nexport default authSlice.reducer","/Users/mac/Afriqana/frontend/src/screens/LoginScreen.jsx",[],[],"/Users/mac/Afriqana/frontend/src/components/FormContainer.jsx",[],[],"/Users/mac/Afriqana/frontend/src/slices/usersApiSlice.js",[],[],"/Users/mac/Afriqana/frontend/src/screens/RegisterScreen.js",[],[],"/Users/mac/Afriqana/frontend/src/screens/DeliveryScreen.jsx",[],[],"/Users/mac/Afriqana/frontend/src/components/PrivateRoute.jsx",[],[],"/Users/mac/Afriqana/frontend/src/components/CheckoutSteps.jsx",[],[],"/Users/mac/Afriqana/frontend/src/screens/PaymentScreen.jsx",[],[],"/Users/mac/Afriqana/frontend/src/screens/PlaceOrderScreen.jsx",[],[],"/Users/mac/Afriqana/frontend/src/slices/ordersApiSlice.js",[],[],"/Users/mac/Afriqana/frontend/src/screens/ProductCatalogScreen.jsx",[],[],"/Users/mac/Afriqana/frontend/src/screens/OrderScreen.jsx",[],[],{"ruleId":"181","severity":1,"message":"182","line":1,"column":26,"nodeType":"183","messageId":"184","endLine":1,"endColumn":46},{"ruleId":"185","severity":1,"message":"186","line":8,"column":3,"nodeType":"187","messageId":"188","endLine":10,"endColumn":4},{"ruleId":"181","severity":1,"message":"189","line":1,"column":10,"nodeType":"183","messageId":"184","endLine":1,"endColumn":14},{"ruleId":"181","severity":1,"message":"190","line":2,"column":23,"nodeType":"183","messageId":"184","endLine":2,"endColumn":34